{
  "name": "hello_swap",
  "version": "1.0.0",
  "description": "A Hello World COMIT app that does a swap",
  "main": "index.js",
  "scripts": {
    "postinstall": "git config core.hooksPath .githooks; yarn run gen",
    "check": "tsc",
    "gen": "mkdir -p ./gen && json2ts -i ./schema/siren.schema.json -o ./gen/siren.d.ts",
    "start": "ts-node -- ./src/index.ts",
    "start-dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "fix": "tslint -p . --fix && prettier --write '**/*.{ts,js,json,yml}'",
    "lint": "tslint -p .",
    "prettier": "prettier --check '**/*.{ts,js,json,yml}'",
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "json-schema-to-typescript": "^7.0.0",
    "prettier": "^1.18.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
